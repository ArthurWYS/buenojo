    <cache name="com.ciis.buenojo.domain.Enrollment" eternal="false"
            overflowToDisk="false">
    </cache>
    
    <cache name="com.ciis.buenojo.domain.UserProfile" eternal="false"
            overflowToDisk="false">
    </cache>
    
    <cache name="com.ciis.buenojo.domain.Course" eternal="false"
            overflowToDisk="false">
    </cache>
    <cache name="com.ciis.buenojo.domain.CourseLevelMap" eternal="false"
            overflowToDisk="false">
    </cache>
    
